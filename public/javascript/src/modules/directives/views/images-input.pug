include _auto-generated/_-all
.gallery
    .file-input(method='post')
    .images-nav
        .md-button.md-primary.md-fab.md-mini.btn-add(ng-click="upload()")
            md-icon.material-icons add
    slick(settings="slider.options" ng-if="slider.state.isReady")
        .slide(ng-repeat="(idx, url) in propData.val track by idx")
            img.slide__img(ng-src="{{url}}", alt="{{url}}")
            .md-button.md-primary.md-fab.md-mini.btn-remove(ng-click="removePhotoByProps(url, idx)")
                md-icon.material-icons clear
